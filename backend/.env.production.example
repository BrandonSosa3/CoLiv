# Database (Railway will provide this)
DATABASE_URL=postgresql://user:password@host:port/dbname

# Security
SECRET_KEY=your-super-secret-key-min-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# CORS (Add your Vercel domains)
FRONTEND_URL=https://co-liv.vercel.app
TENANT_FRONTEND_URL=https://co-liv-tenant.vercel.app

# Environment
ENVIRONMENT=production

# File Storage - Cloudflare R2
R2_ENDPOINT_URL=https://your-account-id.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-production-access-key
R2_SECRET_ACCESS_KEY=your-production-secret-key
R2_BUCKET_NAME=colivos-documents
